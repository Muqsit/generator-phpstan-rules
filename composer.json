{
    "name": "muqsit/generator-phpstan-rules",
    "type": "phpstan-extension",
    "description": "PHPStan rules bringing added Generator inspections",
    "license": ["MIT"],
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Muqsit\\": "src/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}